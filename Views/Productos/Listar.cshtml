
@model List<Producto>

<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Producto</th>
      <th scope="col">Precio</th>
    </tr>
  </thead>
  <tbody>
    @foreach(Producto producto in Model)
    {
        
        <tr>
            <th scope="row"></th>
            <td>@producto.IdProducto</td>      
            <td>@producto.Descripcion</td>
            <td>@producto.Precio</td>
        </tr>    
    }
  </tbody>
</table>